{
  "name": "@robb_j/chowchow-jwt",
  "version": "0.0.0",
  "description": "Adds jwt signing and verification to chowchow",
  "author": "Rob Anderson (https://r0b.io)",
  "repository": "robb-j/chowchow",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "coverage": "jest --coverage",
    "prettier": "prettier --write '**/*.{js,json,css,md,ts,tsx}'",
    "preversion": "npm run test -s && npm run build",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript",
    "modular",
    "chowchow",
    "jwt",
    "jsonwebtoken"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "valid-env": "^1.0.1"
  },
  "devDependencies": {
    "@robb_j/chowchow": "^0.6.2",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.5",
    "ts-jest": "^24.1.0"
  },
  "peerDependencies": {
    "@robb_j/chowchow": "^0.x.x"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
