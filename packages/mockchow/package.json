{
  "name": "@robb_j/mockchow",
  "version": "0.1.4",
  "description": "A test framework for chowchow",
  "author": "Rob Anderson (https://r0b.io)",
  "repository": "robb-j/chowchow",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "coverage": "jest --coverage",
    "prettier": "prettier --write '**/*.{js,json,css,md,ts,tsx}'",
    "preversion": "npm run test -s && npm run build",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript",
    "modular",
    "chowchow"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "path-to-regexp": "^6.1.0"
  },
  "devDependencies": {
    "@robb_j/chowchow": "^0.9.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/stoppable": "^1.1.0",
    "@types/supertest": "^2.0.8"
  },
  "peerDependencies": {
    "@robb_j/chowchow": "^0.x.x"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
